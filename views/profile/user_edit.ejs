<h2><%= titleE %></h2>

<form action="/profile/<%= user._id %>/edit/" method="POST" enctype="multipart/form-data">

  <input  id="lat" name="lat" placeholder="Latitude" style="visibility:hidden">
  <input  id="lon" name="lon" placeholder="Longitude" style="visibility:hidden">
  <br><br>
  <label for="first_name">First Name</label>
  <input id="first_name" type="text" name="first_name" value="<%= user.first_name %>">
  <br><br>
  <label for="last_name">Last Name</label>
  <input id="last_name" type="text" name="last_name" value="<%= user.last_name %>">
  <br><br>
  <label for="username">Username</label>
  <input id="username" type="text" name="username" value="<%= user.username %>">
  <br><br>
  <label for="email">Email</label>
  <input id="email" type="text" name="email" value="<%= user.email %>">
  <br><br>
  <label for="phone">Phone</label>
  <input id="phone" type="number" name="phone" value="<%= user.phone %>">
  <br><br>
  <label for="avatar">photo</label>
  <input id="avatar" type="file" name="avatar" value="<%= user.avatar %>">


  <% if (typeof message !== "undefined") { %>
    <div class="error-message"><%= message %></div>
  <% } %>
  <br><br>

  <button>Edit account</button>

  <p class="account-message">
    <a href="/profile/<%= user._id %>">volver</a>
    <a href="/">Home</a>
  </p>
</form>

<input id="pac-input" class="controls" type="text"
    placeholder="Enter a location">
<div id="map" style="height:350px; width:100%"></div>


<script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAQ-v885dJaXvKNjk-ge5sbkuxWBg_LTD0&libraries=places"></script>
<script type="text/javascript" src="/javascripts/map.js"></script>
